<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <display-name>Example</display-name>

    <servlet>
        <servlet-name>HelloServlet</servlet-name>
        <servlet-class>HelloWorld</servlet-class>
    </servlet>

    <!-- Map URLs to that servlet. -->
    <servlet-mapping>
        <servlet-name>HelloServlet</servlet-name>
        <url-pattern>/servlet</url-pattern>
    </servlet-mapping>
</web-app>